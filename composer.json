    "version": "0.1.0-alpha.1",
    "type": "craft-plugin",
    "homepage": "https://github.com/clearbold/craft-campaignmonitor-campaigns",
    "license": "proprietary",
    "authors": [
        {
            "name": "Mark Reeves",
            "homepage": "http://clearbold.com"
        }
    ],
    "keywords": [
        "craft",
